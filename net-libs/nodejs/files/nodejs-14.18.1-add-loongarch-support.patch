+    CHECK_EQ(tc.fd_fmadd, res_fmadd);
+    CHECK_EQ(tc.fd_fmsub, res_fmsub);
+    CHECK_EQ(tc.fd_fnmadd, res_fnmadd);
+    CHECK_EQ(tc.fd_fnmsub, res_fnmsub);
+  }
