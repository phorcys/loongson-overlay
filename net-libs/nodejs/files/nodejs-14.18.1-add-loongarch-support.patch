+  const int kNumbers[] = {
+      0,   // zero_reg
+      1,   // r1 ra
+      2,   // r2 gp
+      3,   // r3 sp
+      4,   // a0 v0
+      5,   // a1 v1
+      6,   // a2
+      7,   // a3
+      8,   // a4
