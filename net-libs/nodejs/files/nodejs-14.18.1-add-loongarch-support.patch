+  __ li(t2, 1);
+  __ li(t3, 1);
+  __ St_d(t0, MemOperand(a0, offsetof(Test, bold)));
+  __ St_d(t1, MemOperand(a0, offsetof(Test, bold1)));
+  __ St_w(t2, MemOperand(a0, offsetof(Test, dold)));
+  __ St_w(t3, MemOperand(a0, offsetof(Test, dold1)));
+  __ Movz(t0, a4, a6);
